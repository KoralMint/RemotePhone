{
    "theme": {
        "base_theme": "dark"
    },
    "trackpad": {
        "sensitivity": 1.0,
        "acceleration": 1.5,
        "scrollSpeed": 3.0,
        "doubleClickSpeed": 500
    },
    "layout": [
        [
            {
                "label": "Volume",
                "type": "slider",
                "command": "volume_change",
                "multiplier": 0.2,
                "horizontal": false,
                "tap": "mute",
                "position": {
                    "x": 3,
                    "y": 2
                },
                "size": {
                    "width": 1,
                    "height": 2
                }
            },
            {
                "label": "Up",
                "type": "action",
                "command": "up",
                "position": {
                    "x": 2,
                    "y": 3
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Down",
                "type": "action",
                "command": "down",
                "position": {
                    "x": 2,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Left",
                "type": "action",
                "command": "left",
                "position": {
                    "x": 1,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Right",
                "type": "action",
                "command": "right",
                "position": {
                    "x": 3,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Ctrl",
                "type": "action",
                "command": "ctrl",
                "position": {
                    "x": 1,
                    "y": 3
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Shift",
                "type": "action",
                "command": "shift",
                "position": {
                    "x": 1,
                    "y": 2
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Alt",
                "type": "action",
                "command": "alt",
                "position": {
                    "x": 2,
                    "y": 2
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Play/Pause",
                "type": "action",
                "command": "play_pause",
                "position": {
                    "x": 2,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "PrevTrack",
                "type": "action",
                "command": "prev_track",
                "position": {
                    "x": 1,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "NextTrack",
                "type": "action",
                "command": "next_track",
                "position": {
                    "x": 3,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            }
        ],
        [
            {
                "label": "PrevDesktop",
                "type": "action",
                "command": "prev_virtual_desktop",
                "position": {
                    "x": 1,
                    "y": 3
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "NextDesktop",
                "type": "action",
                "action": "action",
                "command": "next_virtual_desktop",
                "position": {
                    "x": 3,
                    "y": 3
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Volume",
                "type": "slideaction",
                "command": {
                    "positive": ["shift", "ctrl", "tab"],
                    "negative": ["ctrl", "tab"]
                },
                "threshold": 30,
                "tap": "mute",
                "position": {
                    "x": 2,
                    "y": 2
                },
                "size": {
                    "width": 1,
                    "height": 2
                }
            },
            {
                "label": "Esc",
                "type": "vk",
                "command": [91, 9],
                "position": {
                    "x": 1,
                    "y": 4
                },
                "size": {
                    "width": 3,
                    "height": 1
                }
            }
        ]
    ]
}